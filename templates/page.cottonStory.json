    "vertical_tabs_g8Y4Wh": {
      "type": "vertical-tabs",
      "blocks": {
        "tab_Vft6aP": {
          "type": "tab",
          "settings": {
            "title": "Why Fiber Length Matters",
            "image": "shopify://shop_images/WhatsApp_Image_2025-06-21_at_10.02.20_PM.jpg",
            "image_mobile": "shopify://shop_images/WhatsApp_Image_2025-06-21_at_10.02.20_PM.jpg",
            "text": "Longer fibers make the fabric smoother, stronger, and less prone to pilling. They also absorb dye better, giving richer, longer-lasting color."
          }
        },
        "tab_acJfYK": {
          "type": "tab",
          "settings": {
            "title": "Where It Grows",
            "image": "shopify://shop_images/WhatsApp_Image_2025-06-21_at_10.29.32_PM.jpg",
            "image_mobile": "shopify://shop_images/WhatsApp_Image_2025-06-21_at_10.29.32_PM.jpg",
            "text": "Supima cotton is grown exclusively in the United States, mainly in California, Arizona, Texas, and New Mexico. These regions offer ideal climate and soil conditions for premium cotton cultivation."
          }
        },
        "tab_QBBhcW": {
          "type": "tab",
          "settings": {
            "title": "How It’s Harvested",
            "image": "shopify://shop_images/WhatsApp_Image_2025-06-21_at_10.21.12_PM.jpg",
            "image_mobile": "shopify://shop_images/WhatsApp_Image_2025-06-21_at_10.21.12_PM.jpg",
            "text": "Supima cotton is harvested using advanced, modern machinery that preserves fiber integrity.  This ensures uniform quality and minimizes damage compared to traditional hand-picking methods."
          }
        },
        "tab_Lk6jiE": {
          "type": "tab",
          "settings": {
            "title": "Why Supima Feels Different",
            "image": "shopify://shop_images/WhatsApp_Image_2025-06-21_at_10.16.01_PM.jpg",
            "image_mobile": "shopify://shop_images/WhatsApp_Image_2025-06-21_at_10.16.01_PM.jpg",
            "text": "Supima feels softer because its fibers are finer, longer, and smoother than regular cotton. This results in a luxurious hand-feel and a fabric that drapes better on the body."
          }
        }
      },
